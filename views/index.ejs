    <div class="product-gallery">
    <% for ( var i = 0, x = state.variants.length; i < x; i++ ) { %>
        <amp-carousel id="<%= state.variants[i].slug %>-carousel"
          width="1024"
          height="682"
          layout="responsive"
          type="slides"
          [slide]="product.selectedSlideFor<%= state.variants[i].slug %>"
          on="slideChange: AMP.setState({product: {selectedSlideFor<%= state.variants[i].slug %>: event.index}})"
          class="fadeIn"
          [hidden]="product.selectedVariantIndex != <%= i %>"
          <% if ( i > 0 ) { %>
          hidden
          <% } %>
        >
          <% for (var j = 0, y = state.variants[i].images.length; j < y; j++) { %>
              <amp-img src="<%= state.variants[i].images[j] %>"
                width="1024"
                height="682"
                layout="responsive"
                on="tap:gallery-lightbox"
                role="button"
                tabindex="0">
              </amp-img>
          <% } %>
        </amp-carousel>
    <% } %>

        <amp-image-lightbox
            id="gallery-lightbox"
            layout="nodisplay"
        >
          <div on="tap:gallery-lightbox.close"
            role="button"
            tabindex="0">
            <button class="ampstart-btn caps m2 close-gallery-button"
              on="tap:gallery-lightbox.close"
              role="button"
              tabindex="0">
              Close
            </button>
          </div>
        </amp-image-lightbox>


      </div>


      <amp-state id="product">
        <script type="application/json">
            <%- JSON.stringify( state ) %>
        </script>
      </amp-state>

            <div class="shoppdp-c-productPricing">
