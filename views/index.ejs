                    <amp-selector name="Variant"
                      layout="container"
                      [selected]="product.selectedVariantIndex"
                      on="select:AMP.setState({
                          product: {
                            selectedVariantIndex: event.targetOption,
                            selectedSlideForPeppermint: 0,
                            selectedSlideForYellow: 0,
